      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Visualizing the target variable\n",
    "plt.pie(fraud_data['is_fraud'].value_counts(normalize=True) * 100,labels=['Legit','Fraud'], autopct='%.2f%%',)\n",
    "plt.legend()\n",
